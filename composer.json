{
  "name": "zhortein/seo-tracking-bundle",
  "type": "symfony-bundle",
  "keywords": ["symfony", "symfony-ux", "bundle", "seo", "track", "tracking", "utm", "stats", "statistics"],
  "description": "This bundle is dedicated to Symfony applications and provide metrics gathering on page calls.",
  "homepage": "https://www.david-renard.fr",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    {
      "name": "David Renard",
      "email": "david.renard.21@free.fr",
      "homepage": "https://www.david-renard.fr",
      "role": "Owner, Developer"
    }
  ],
  "require": {
    "php": ">=8.3",
    "doctrine/dbal": "^3|^4",
    "doctrine/doctrine-bundle": "^2.15",
    "doctrine/orm": "^3.3",
    "symfony/asset-mapper": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/string": "^7.3",
    "symfony/translation": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/validator": "^7.3",
    "symfony/yaml": "^7.3"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-doctrine": "^2.0",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/php-code-coverage": "^12.3.1",
    "phpunit/phpunit": "^12.2.5",
    "friendsofphp/php-cs-fixer": "^v3.75.0",
    "symfony/phpunit-bridge": "^7.3",
    "roave/security-advisories": "dev-latest",
    "symfony/test-pack": "^1.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Zhortein\\SeoTrackingBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zhortein\\SeoTrackingBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "php-cs-fixer": "php-cs-fixer fix src --verbose --rules=@Symfony"
  }
}